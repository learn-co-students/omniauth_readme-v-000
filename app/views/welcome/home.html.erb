<%# Add the Facebook login link here %>
<% if session[:user_id] %>
    <h1><%= @user.name %></h1>
    <h2>Email: <%= @user.email %></h2>
    <h3>Facebook UID: <%= @user.uid %></h3>
    <img src="<%= @user.image %>">
    <%= link_to "Logout", '/logout'%>
<% else %>
    <%= link_to('Log in with Facebook!', '/auth/facebook') %>
<% end %>

