<%# Add the Facebook login link here %>
<%= link_to ('Log In with GitHub', '/auth/github') %>

<% if session[:user_id] %>
  <h1><%= @user.name %></h1>
  <h2>Email: <%= @user.email %></h2>
  <h2>GitHub UID: <%= @user.uid %></h2>
  <img src="<%= @user.image %>">
<% else %>
  <%= link_to('Log in with GitHub', '/auth/github') %>
<% end %>
